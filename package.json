{
	"name": "svelte-electricsql",
	"version": "0.1.3",
	"license": "MIT",
	"packageManager": "pnpm@9.4.0",
	"engines": {
		"pnpm": "^9.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/milimyname/svelte-electricsql.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && pnpm package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "pnpm package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "pnpm test:unit -- --run",
		"db:start": "docker compose -f .support/docker-compose.yml up -d",
		"db:reset": "docker compose -f .support/docker-compose.yml down && pnpm db:start",
		"changeset:version": "changeset version",
		"changeset:publish": "pnpm build && changeset publish",
		"release": "pnpm build && changeset publish"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.28.1",
		"@eslint/compat": "^1.2.7",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.17.3",
		"@sveltejs/package": "^2.3.10",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.7",
		"@testing-library/user-event": "^14.6.1",
		"@types/pg": "^8.11.11",
		"eslint": "^9.21.0",
		"eslint-config-prettier": "^10.0.2",
		"eslint-plugin-svelte": "^3.0.2",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"pg": "^8.13.3",
		"prettier": "^3.5.2",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.7",
		"svelte": "^5.20.5",
		"svelte-check": "^4.1.4",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.25.0",
		"vite": "^6.2.0",
		"vitest": "^3.0.7"
	},
	"dependencies": {
		"@electric-sql/client": "1.0.0-beta.3"
	}
}
